{
	"training": {
		"method": "Adam",
		"opt_params": {
			"lr": 1e-4
		},
		"lr_scheduler": "const",
		"batch_size": 512,
		"epochs": 60,
		"steps": -1,
		"gamma_conc": 1.0,
		"gamma_rate": 0.25,
		"field_magnitude": 0.5,
		"t_end": 15,
		"integrator": "rk4",
		"step_size": 0.2
	},
	"data": {
		"dataset": "pinwheel",
		"integer_smoothing": 0.01,
		"batches_per_epoch": 500
	},
	"model": "dense",
	"model_params": {
		"hidden_dims": [256, 256],
		"bias": true
	},
	"logging": {
		"eval_interval_epochs": 1,
		"check_interval_epochs": 10
	}
}